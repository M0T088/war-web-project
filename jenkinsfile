String maven = "maven:3.6.3-adoptopenjdk-14"
pipeline {
    agent none
    stages {
        stage('war File') {
            agent {
                docker {
                    image 'maven'
                    args '--network tomcat_moto_net'
                }
            }
            steps {
                    sh 'mvn package'
                    }
                }
        stage('Deploy War File') {
            steps {
				ansiblePlaybook colorized: true, installation: 'ansible2', playbook: 'dockertest.yml', disableHostKeyChecking: true
            }
        }
    }
}